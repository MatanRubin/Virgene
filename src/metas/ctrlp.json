{
    "name": "CtrlP",
    "feature_type": "Plugin",
    "description": "Full path fuzzy file, buffer, mru, tag, ... finder for Vim.",
    "default_value": false,
    "enabled": true,
    "category": "Plugin",
    "installed": true,
    "template": "ctrlp.j2",
    "vundle_installation": "ctrlpvim/ctrlp.vim",
    "options":
    [
        {
            "name": "ctrlp_cmd",
            "option_type": "Option",
            "pretty_name": "CtrlP Command",
            "description": "Which mode should CtrlP start in",
            "default_value": "CtrlPMixed"
        },
        {
            "name": "ctrlp_extensions",
            "option_type": "Option",
            "pretty_name": "CtrlP Extensions",
            "description": "bla bla",
            "default_value": "['dir', 'mixed']"
        },
        {
            "name": "ctrlp_working_path_mode",
            "option_type": "Choice",
            "choices": ["c", "a", "r", "w", "0"],
            "pretty_name": "CtrlP working path mode",
            "description": "bli bla",
            "default_value": "r"
        }
    ]
}
